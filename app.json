    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["VIBRATE"],
      "package": "io.ciaos.tsuku",
      "versionCode": 4,
    },
    "description": "A shameless Shobu clone."
  },
  "scripts": {
    "postinstall": "patch-package"
  }
}