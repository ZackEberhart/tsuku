{
  "expo": {
    "name": "Tsuku",
    "slug": "playtsuku",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "android",
    ],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
